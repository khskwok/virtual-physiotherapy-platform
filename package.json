{
  "name": "virtual-physiotherapy-platform",
  "version": "1.0.0",
  "description": "Virtual physiotherapy consultations with AI-powered posture analysis for Hong Kong",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "test": "jest",
    "install:all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2"
  },
  "keywords": ["physiotherapy", "telemedicine", "webrtc", "ai", "hong-kong", "cantonese"],
  "author": "Virtual Physiotherapy Team",
  "license": "MIT"
}